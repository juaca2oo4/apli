<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="178.0" prefWidth="106.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.servermanagement.controller.AddServerController">
   <children>
      <VBox alignment="CENTER" prefHeight="225.0" prefWidth="200.0" spacing="10">
         <children>
            <Label text="Server Name" />
            <TextField fx:id="serverName" alignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#onAddServer" text="Confirm" />
            <Button mnemonicParsing="false" onAction="#onClose" text="Close" />
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
