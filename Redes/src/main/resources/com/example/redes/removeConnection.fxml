<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="191.0" prefWidth="207.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.servermanagement.controller.RemoveConnectionController">
    <children>
        <VBox prefHeight="188.0" prefWidth="200.0" spacing="5">
            <children>
                <Label text="Server 1">
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin></Label>
                <TextField fx:id="originServer" prefHeight="20" prefWidth="250">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin></TextField>
                <Label text="Server 2">
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin></Label>
                <TextField fx:id="goalServer" prefHeight="20" prefWidth="250">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin></TextField>
                <HBox alignment="CENTER" prefHeight="348.0" prefWidth="491.0" spacing="40">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onAddConnection" text="Confirm" />
                        <Button mnemonicParsing="false" onAction="#onClose" text="Close" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
